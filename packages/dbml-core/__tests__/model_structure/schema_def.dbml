Table ecommerce.users {
  id int [pk]
  name varchar
  js job_status
  js2 public.job_status
  g schemaB.gender
}

Table public.users {
  id int [pk]
  name varchar
}

Table products {
  id int [pk]
  name varchar
}

Table schemaA.products {
  id int [pk]
  name varchar [ref: > ecommerce.users.id]
}

Table schemaA.locations {
  id int [pk]
  name varchar [ref: > users.id]
}

Ref: public.users.id < ecommerce.users.id

Ref name_optional {
  users.name < ecommerce.users.id
}

TableGroup tablegroup_name { // tablegroup is case-insensitive.
  public.products 
  users
  ecommerce.users
}

enum job_status {
    created [note: 'Waiting to be processed']
    running
    done
    failure
}

enum schemaB.gender {
    male
    female
}